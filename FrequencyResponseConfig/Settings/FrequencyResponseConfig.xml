    </Stimulus>

    <MeasuringPoint>
      <Location Tag="AxisSignals.CurrentControllerOutputVoltageQ"/>
    </MeasuringPoint>

    <MeasuringPoint>
      <Location Tag="AxisSignals.CurrentControllerActualCurrentQ"/>
